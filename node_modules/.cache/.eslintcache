/ return the JSX for ingredient: count times\n    });\n  }).reduce( (arr , el)=>\n  {\n    return arr.concat(el)\n  }, [])\n  console.log(transformedIngredients)\n if(transformedIngredients.length === 0)\n {\n   transformedIngredients = <p> Please add something</p>\n }\n    return (\n        <div className= \"Burger\">\n      <BurgerIngredient type=\"bread-top\"/>\n      {transformedIngredients}\n      <BurgerIngredient type=\"bread-bottom\"/>\n  \n        </div>\n    );\n\n}\nexport default Burger;","/Users/ashutoshtripathi/Desktop/burger-project/src/components/Burger/BurgerIngredient/BurgerIngredient.js",[],"/Users/ashutoshtripathi/Desktop/burger-project/src/components/Burger/BuildControls/BuildControls.js",[],"/Users/ashutoshtripathi/Desktop/burger-project/src/components/Burger/BuildControls/BuildControl/BuildControl.js",[],"/Users/ashutoshtripathi/Desktop/burger-project/src/components/UI/Modal/Modal.js",[],"/Users/ashutoshtripathi/Desktop/burger-project/src/components/Burger/OrderSummary/OrderSummary.js",[],"/Users/ashutoshtripathi/Desktop/BurgerProject/src/index.js",[],["146","147"],"/Users/ashutoshtripathi/Desktop/BurgerProject/src/App.js",["148"],"/Users/ashutoshtripathi/Desktop/BurgerProject/src/reportWebVitals.js",[],"/Users/ashutoshtripathi/Desktop/BurgerProject/src/containers/BurgerBuilder/BurgerBuilder.js",["149","150"],"import React, {useState, Component} from 'react'\nimport Aux from '../../hoc/Aux'\nimport Burger from '../../components/Burger/Burger'\nimport BuildControls from '../../components/Burger/BuildControls/BuildControls'\nimport Modal from '../../components/UI/Modal/Modal';\nimport OrderSummary from '../../components/Burger/OrderSummary/OrderSummary';\nimport axios from \"../../axios-orders\";\n\nconst ingredient_prices = {\n\n    salad : 0.5,\n    cheese : 0.4,\n    meat : 1.3,\n    bacon : 0.7\n};\n\nclass BurgerBuilder extends Component {\n    state = {\n        ingredients:{\n            salad : 0,\n            bacon :0 ,\n            cheese : 0,\n            meat: 0\n        },\n        totalPrice : 4,\n        purchasable : false,\n        purchasing : false\n    }\n\n    updatePurchasable (ingredients)\n    {\n       \n        const sum = Object.keys(ingredients).map(igKey =>{\n            return ingredients[igKey];\n        }).reduce((sum , el) =>{\n            return sum +el;\n\n        },0);\n\n       this.setState({purchasable: sum > 0})\n    }\n\n     purchaseHandler = ()=>\n     {\n         this.setState({purchasing : true});\n     }\n     purchaseCancel = ()=>\n     {\n      this.setState({purchasing : false})\n     }\n\n     purchaseContinue =()=>\n     {\n         const order = {\n             ingredients: this.state.ingredients,\n             price : this.state.totalPrice,\n             customer: {\n                 name:'Ashutosh',\n                 address: {\n                     street: 'Teststreet 1',\n                     zipcode : '41343'\n                 }\n             }\n         }\n         axios.post('/orders.json', order).then(response=>{\n             console.log(response);\n         }).catch(error=>{\n             console.log(error);\n         });\n\n     }\n    addIngredientHandler = (type) =>\n    {\n        const oldCount = this.state.ingredients[type];\n        const updatedCounted  = oldCount + 1;\n        const updatedIngredients =  {\n            ...this.state.ingredients\n        };\n        updatedIngredients[type] = updatedCounted;\n        const priceAddition = ingredient_prices[type];\n        const oldPrice = this.state.totalPrice ;\n        const newPrice = oldPrice + priceAddition;\n        this.setState({\n            totalPrice: newPrice , ingredients : updatedIngredients\n        })\n        this.updatePurchasable(updatedIngredients);\n        \n        \n    }\n    removeIngredientHandler = (type) =>\n    {\n       \n        const oldCount = this.state.ingredients[type];\n        if(oldCount !== 0)\n        {\n\n        \n        const updatedCounted  = oldCount - 1;\n        const updatedIngredients =  {\n            ...this.state.ingredients\n        };\n        updatedIngredients[type] = updatedCounted;\n        const priceAddition = ingredient_prices[type];\n        const oldPrice = this.state.totalPrice ;\n        const newPrice = oldPrice - priceAddition;\n        this.setState({\n            totalPrice: newPrice , ingredients : updatedIngredients\n        })\n        this.updatePurchasable(updatedIngredients);\n    }\n\n     }\n    \n    render() {\n    {\n          const disabledInfo = {\n              ...this.state.ingredients\n          };\n          \n          for(let key in disabledInfo)\n          {\n              disabledInfo[key] = disabledInfo[key] <=0\n          }\n        return (\n            <Aux>\n                <Modal show = {this.state.purchasing} modalClosed ={this.purchaseCancel} > \n                    <OrderSummary ingredients = {this.state.ingredients}\n                     continue = {this.purchaseContinue}\n                     cancel = {this.purchaseCancel}\n                     price = {this.state.totalPrice}/>\n                </Modal>\n                <Burger ingredients = {this.state.ingredients}/>\n                <BuildControls \n                   ingredientsAdded = {this.addIngredientHandler}\n                   ingredientremoved = {this.removeIngredientHandler} \n                   disabled = {disabledInfo}\n                   price = {this.state.totalPrice} \n                   purchasable= {this.state.purchasable}\n                   ordered = {this.purchaseHandler}\n                   /> \n            </Aux>\n        );\n    }\n}\n}\nexport default BurgerBuilder;\n","/Users/ashutoshtripathi/Desktop/BurgerProject/src/components/Layout/Layout.js",[],"/Users/ashutoshtripathi/Desktop/BurgerProject/src/hoc/Aux.js",[],"/Users/ashutoshtripathi/Desktop/BurgerProject/src/components/Burger/Burger.js",["151"],"/Users/ashutoshtripathi/Desktop/BurgerProject/src/components/UI/Modal/